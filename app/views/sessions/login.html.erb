<%= render 'shared/content_title', title: "Log in" %>
<%= form_tag '/login' do %>
<div class="well">
  <div class="control-group">
    <%= label_tag 'username' %>
    <%= text_field_tag 'username' %>
  </div>
  <div class="control-group">
    <%= label_tag 'password' %>
    <%= password_field_tag %>
  </div>
  <%= submit_tag 'Log in', class: 'btn btn-success' %>
</div>
<% end %>