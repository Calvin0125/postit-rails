<%= render 'shared/content_title', title: "Log in" %>
<%= render 'shared/errors', resource: @user %>
<%= form_for @user do |f| %>
  <div class="well">
    <div class="control-group">
      <%= f.label :username %>
      <%= f.text_field :username %>
    </div>
    <div class="control-group">
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

    <%= f.submit "Register", class: "btn btn-success" %>
  </div>
<% end %>